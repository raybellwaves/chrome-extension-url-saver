{
    "manifest_version": 3,
    "name": "Save URL to Google Sheet",
    "version": "1.0",
    "description": "Save the current page's URL to a Google Sheet.",
    "permissions": ["tabs"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    }
 }
 